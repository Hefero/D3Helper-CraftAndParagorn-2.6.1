<Window x:Class="Enigma.D3.MemoryExplorer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enigma.D3.MemoryExplorer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <WrapPanel DockPanel.Dock="Top">
            <TextBlock Text="Address" />
            <TextBox Name="UXAddress" Width="80" Text="0x14187E7B0" />
            <TextBlock Text="Size" Margin="20,0,0,0" />
            <TextBox Name="UXSize" Width="80" Text="0x8" />
            <Button Content="Ptr Analyze" Margin="20,0,0,0" Width="80" Click="Button_Click" />
            <Button Content="Full Analyze" Margin="20,0,0,0" Width="80" Click="Button_Click_2" />
            <Button Content="Back" Margin="20,0,0,0" Width="80" Click="Button_Click_1" />
        </WrapPanel>
        <TextBlock Name="UXHeader" FontFamily="Consolas" DockPanel.Dock="Top" />
        <ContentPresenter Name="UXView" />
    </DockPanel>
</Window>
